{
  "name": "tashanstone",
  "private": true,
  "version": "1.7.8",
  "description": "TashanStone - AI-powered Markdown Editor",
  "author": {
    "name": "TashanStone Team",
    "email": "tashanstone@example.com"
  },
  "homepage": "https://github.com/tashanstone/tashanstone",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist-electron/electron/main.js",
  "scripts": {
    "dev": "vite",
    "dev:electron": "concurrently -k \"vite --port 5173 --strictPort\" \"wait-on tcp:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 node scripts/run-electron-dev.cjs\"",
    "build": "tsc && vite build",
    "build:electron": "cross-env ELECTRON=true npm run build && tsc -p electron/tsconfig.json && tsc -p electron/tsconfig.preload.json && node -e \"require('fs').renameSync('dist-electron/electron/preload.js','dist-electron/electron/preload.cjs')\"",
    "preview": "vite preview",
    "dist": "npm run build:electron && electron-builder",
    "dist:win": "npm run build:electron && electron-builder --win",
    "dist:win:x64": "npm run build:electron && electron-builder --win --x64",
    "dist:win:arm64": "npm run build:electron && electron-builder --win --arm64",
    "dist:mac": "npm run build:electron && electron-builder --mac",
    "dist:linux": "npm run build:electron && electron-builder --linux",
    "dist:linux:deb": "npm run build:electron && electron-builder --linux deb",
    "dist:linux:rpm": "npm run build:electron && electron-builder --linux rpm",
    "dist:linux:x64": "npm run build:electron && electron-builder --linux --x64",
    "dist:linux:arm64": "npm run build:electron && electron-builder --linux --arm64",
    "dist:linux:deb:x64": "npm run build:electron && electron-builder --linux deb --x64",
    "dist:linux:deb:arm64": "npm run build:electron && electron-builder --linux deb --arm64",
    "dist:linux:rpm:x64": "npm run build:electron && electron-builder --linux rpm --x64",
    "dist:linux:rpm:arm64": "npm run build:electron && electron-builder --linux rpm --arm64",
    "dist:kylin": "npm run build:electron && electron-builder --linux deb --x64 --arm64",
    "dist:all": "npm run build:electron && electron-builder --win --linux",
    "postinstall": "electron-builder install-app-deps",
    "download:sherpa-model": "node scripts/download-sherpa-model.cjs",
    "download:ocr-model": "node scripts/download-ocr-model.cjs",
    "bun:dev": "bun run vite",
    "bun:dev:electron": "bun run concurrently -k \"vite --port 5173 --strictPort\" \"wait-on tcp:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 node scripts/run-electron-dev.cjs\"",
    "bun:build": "bun run tsc && bun run vite build",
    "bun:build:electron": "cross-env ELECTRON=true bun run bun:build && bun run tsc -p electron/tsconfig.json && bun run tsc -p electron/tsconfig.preload.json && bun -e \"require('fs').renameSync('dist-electron/electron/preload.js','dist-electron/electron/preload.cjs')\"",
    "bun:dist": "bun run bun:build:electron && bun run electron-builder",
    "bun:dist:win": "bun run bun:build:electron && bun run electron-builder --win",
    "bun:dist:mac": "bun run bun:build:electron && bun run electron-builder --mac",
    "bun:dist:linux": "bun run bun:build:electron && bun run electron-builder --linux",
    "bun:dist:linux-x64": "bun run bun:build:electron && bun run electron-builder --linux --x64",
    "bun:dist:linux-arm64": "bun run bun:build:electron && bun run electron-builder --linux --arm64",
    "bun:dist:all": "bun run bun:build:electron && bun run electron-builder --win --linux",
    "bun:test": "bun run vitest run",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@codemirror/commands": "^6.10.1",
    "@codemirror/lang-markdown": "^6.5.0",
    "@codemirror/language": "^6.12.1",
    "@codemirror/state": "^6.5.3",
    "@codemirror/view": "^6.39.8",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@fontsource/ma-shan-zheng": "^5.2.8",
    "@fontsource/noto-sans-sc": "^5.2.8",
    "@fontsource/noto-serif-sc": "^5.2.8",
    "@google/genai": "^1.30.0",
    "@lancedb/lancedb": "^0.23.0",
    "@paddlejs-models/ocr": "^1.2.4",
    "@paddlejs/paddlejs-core": "^2.2.0",
    "@techstark/opencv-js": "^4.12.0-release.1",
    "@types/diff": "^7.0.2",
    "@types/papaparse": "^5.5.2",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@uiw/react-codemirror": "^4.25.4",
    "better-sqlite3": "^12.6.0",
    "canvas": "^3.2.0",
    "clsx": "^2.1.0",
    "d3": "^7.9.0",
    "diff": "^8.0.2",
    "esearch-ocr": "^8.5.0",
    "fluent-ffmpeg": "^2.1.3",
    "js-clipper": "^1.0.1",
    "lucide-react": "^0.555.0",
    "mammoth": "^1.11.0",
    "mermaid": "^11.12.1",
    "onnxruntime-node": "^1.23.2",
    "onnxruntime-web": "^1.23.2",
    "papaparse": "^5.5.3",
    "pdfjs-dist": "^5.4.449",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-virtualized-auto-sizer": "^2.0.1",
    "react-window": "^2.2.3",
    "rehype-highlight": "^7.0.2",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "sherpa-onnx": "^1.12.19",
    "sherpa-onnx-node": "^1.12.19",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.12",
    "@types/d3": "^7.4.3",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/node": "^22.19.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.17",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "33.4.11",
    "electron-builder": "^25.1.8",
    "electron-icon-builder": "^2.0.1",
    "jimp": "^1.6.0",
    "jsdom": "^27.4.0",
    "playwright-core": "^1.52.0",
    "png-to-ico": "^3.0.1",
    "postcss": "^8.5.6",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "to-ico": "^1.1.5",
    "typescript": "~5.9.3",
    "vite": "^6.2.0",
    "vite-plugin-static-copy": "^3.1.6",
    "vitest": "^4.0.18",
    "wait-on": "^8.0.1"
  }
}
